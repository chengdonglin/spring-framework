<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.dmai.spring.bean.Person" id="person">
		<property name="name" value="lcd"></property>
	</bean>
	<bean id="myBeanFactoryProcessor" class="com.dmai.spring.bean.MyBeanFactoryPostProcessor" >

	</bean>

	<bean id="personPostProcessor" class="com.dmai.spring.bean.PersonPostProcessor"></bean>
</beans>